  "license": "MIT",
  "dependencies": {
    "date-fns": "^2.28.0",
    "node-fetch": "^2.6.7",
    "processmd": "^4.6.3",
    "qiita-js": "^0.4.3"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "husky": "^8.0.1",
    "json-schema-to-typescript": "^11.0.1",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
